@{
  Layout = "_Layout";
}
@using Salon.Models.Stylist
@using Salon.Models

@if(Model.Count == 0)
{
  <h4>There are no Stylists</h4>
}
<ul>
@ForEach(Stylist stylist of Model)
{
  <li><h4>@Html.ActionLink($"{stylist.Name}",
  "Show", new { id == stylist.StylistId})</h4></li>
}
</ul>
@Html.ActionLink(Home, "Index", "Home")

