@{
  Layout = "_Layout";
}
@using Salon.Models;
@model List<Salon.Models.Stylist>;

@if (@Model.Count == 0)
{
  <h4>There are no Stylists</h4>
}
<ul>
@foreach (Stylist stylist in Model)
{
  <li>@Html.ActionLink($"{stylist.Name}", "Show", new { id = stylist.StylistId})</li>
}
</ul>
  
<p>@Html.ActionLink("Home", "Index", "Home")</p>

}
</ul>

@Html.ActionLink("Home", "Index", "Home")

